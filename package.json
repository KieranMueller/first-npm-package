{
    "name": "nearest-prime",
    "version": "1.0.9",
    "description": "nearestPrime() takes a number and returns the closest prime number",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "npm run build-common && npm run build-es",
        "build-common": "tsc --module commonjs --outDir ./dist/cjs",
        "build-es": "tsc --module esnext --outDir ./dist/esm",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KieranMueller/nearest-prime-npm.git"
    },
    "keywords": [
        "closest",
        "nearest",
        "prime",
        "number",
        "utility"
    ],
    "author": "Kieran Mueller",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/KieranMueller/nearest-prime-npm/issues"
    },
    "homepage": "https://github.com/KieranMueller/nearest-prime-npm#readme",
    "devDependencies": {
        "typescript": "^5.7.2"
    }
}
