{
    "name": "nearest-prime",
    "version": "1.0.14",
    "description": "nearestPrime() takes a number and returns the closest prime number",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "scripts": {
        "build": "npm run build-common && npm run build-es",
        "build-common": "tsc --project tsconfig.commonjs.json",
        "build-es": "tsc --project tsconfig.esm.json",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KieranMueller/nearest-prime.git"
    },
    "keywords": [
        "closest",
        "nearest",
        "prime",
        "number",
        "utility"
    ],
    "author": "Kieran Mueller",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/KieranMueller/nearest-prime/issues"
    },
    "homepage": "https://github.com/KieranMueller/nearest-prime#readme",
    "devDependencies": {
        "typescript": "^5.7.2"
    }
}
